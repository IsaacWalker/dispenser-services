@model Web.Models.ViewModels.NavbarPartialModel
@*
    
*@
<div id="layoutSidenav_nav">
        <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
            <div class="sb-sidenav-menu">
                <div class="nav">
                    <div class="sb-sidenav-menu-heading">Navigate</div><a class="nav-link" href="/index">
                        <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>Dashboard
                    </a>
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLayouts" aria-expanded="false" aria-controls="collapseLayouts">
                        <div class="sb-nav-link-icon"><i class="fas fa-user"></i></div>Patients
                        <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                    </a>
                    <div class="collapse" id="collapseLayouts" aria-labelledby="headingOne" data-parent="#sidenavAccordion">
                        <nav class="sb-sidenav-menu-nested nav">
                            <a class="dropdown-item"><strong> Batch @Model.BatchNumber </strong></a>
                            <div class="dropdown-divider"></div>
                            @for (int i = 0; i < Model.ODFs.Count; i++)
                            {            
                                <a class="nav-link" href="/patient?nurseId=@Model.NurseId&patientId=@Model.ODFs[i].PatientId">
                                    @Model.ODFs[i].PatientName
                                </a>
                            }
                        </nav>
                    </div>
                </div>
            </div>
            <div class="sb-sidenav-footer">
                <div class="small">Logged in as:</div><strong>@Model.NurseFirstName @Model.NurseLastName</strong>
            </div>
        </nav>
    </div>
